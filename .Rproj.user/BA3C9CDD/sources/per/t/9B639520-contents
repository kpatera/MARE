      delta[i] ~ dnorm(d, prec)
      }
      OR <- exp(d)
      d ~ dnorm(0.0,pControl) 
      lv ~ dunif(-10,1.386)
      log(tau.sq)<-lv
      prec<-1/(tau.sq)
      tau<-sqrt(tau.sq)
      d.new ~dnorm(d,prec)
}", file="betaMeta2.txt")
} # log(tau2) ~ U(-10,1.386) 
